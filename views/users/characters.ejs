<%- include('../partials/_html-head.ejs') %>
        <link rel="stylesheet" href="/stylesheets/style.css">
        <title><%= user.username %>'s Roster</title>
    </head>

        <%- include('../partials/_navbar.ejs') %>
        <div id="border-d-scale"><div id="border-d"><div id="bordered-d"></div></div></div>
        <h1 class="header-one"><%= user.username %></h1>
        <div id="content-container-top">
            <ul>
            <% user.characters.forEach((character) => { %>
                <div class="character-card">
                    <li class="name-text">
                        <%= character.name %> |
                        Level <%= character.level %>
                    </li>
                </div>
            <% }); %>
            </ul>
        </div>

    </body>

</html>